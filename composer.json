{
    "name": "liuggio/fastest",
    "license": "MIT",
    "authors": [
        {
            "name": "liuggio",
            "email": "liuggio@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "~2.3",
        "symfony/stopwatch": "~2.3",
        "symfony/process": "~2.3",
        "doctrine/collections" : "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "predis/predis": "~0.8.0"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "phpunit/phpunit": "Install it if you want to run test on /bin/phpunit"
    },
    "bin": [
        "fastest"
    ],
    "autoload": {"psr-4": {"Liuggio\\Fastest\\": "src"}}
}
